{
    "name": "juling/think-devtools",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.3",
        "topthink/framework": "^8.0"
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Juling\\DevTools\\": "src/"
        }
    },
    "extra": {
        "think": {
            "services": [
                "Juling\\DevTools\\DevToolsServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
